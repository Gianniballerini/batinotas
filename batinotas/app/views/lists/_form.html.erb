<%= simple_form_for @list, class: "form-horizontal"  do |f| %>
  <%= f.error_notification %>
	<div class="form-group form-group-lg	">
		<div class="col-sm-10">
			<%= f.input :name, label: false, input_html: { class: 'form-control' }, placeholder: "Nombre de la Lista" %>
		</div>
	</div>
	<%= f.button :submit , class: "btn btn-success"%>
<% end %>

